{
  "name": "purple-dbu/vcloud-sdk-helpers",
  "description": "Utility classes for vCloud Director PHP SDK",
  "type": "library",
  "keywords": [
    "vmware",
    "vcloud",
    "director",
    "api",
    "sdk",
    "helpers"
  ],
  "homepage": "https://github.com/purple-dbu/vcloud-sdk-php-helpers",
  "license": "MIT",
  "require": {
    "php": ">=5.3.2"
  },
  "suggest": {
    "pear-pear/HTTP_Request2": "Requires HTTP_Request2 from pear's repository http://pear.php.net if it is not installed system-wise"
  },
  "autoload": {
    "psr-0": {
      "VMware\\": "src/"
    }
  },
  "include-path": [
    "src/"
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.php.net"
    }
  ],
  "require-dev": {
    "vmware/vcloud-sdk-patched": "*",
    "pear-pear/HTTP_Request2": "*"
  }
}
