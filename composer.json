{
    "name": "purple-dbu/vcloud-sdk-helpers",
    "description": "Utility classes for vCloud Director PHP SDK",
    "type": "library",
    "keywords": [
        "vmware",
        "vcloud",
        "director",
        "api",
        "sdk",
        "helpers"
    ],
    "homepage": "https://github.com/purple-dbu/vcloud-sdk-php-helpers",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "VCloud\\Helpers\\": "src/"
        }
    },
    "include-path": [
        "src/"
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "vmware/vcloud-sdk-patched": "*",
        "pear-pear/HTTP_Request2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*",
        "satooshi/php-coveralls": ">=0.6.1",
        "pear-pear/PHP_CodeSniffer": ">=1.4.5"
    }
}
