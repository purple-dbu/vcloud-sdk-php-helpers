{
    "name": "purple-dbu/vcloud-sdk-helpers",
    "description": "Utility classes for vCloud Director PHP SDK",
    "type": "library",
    "keywords": [
        "vmware",
        "vcloud",
        "director",
        "api",
        "sdk",
        "helpers"
    ],
    "homepage": "https://github.com/purple-dbu/vcloud-sdk-php-helpers",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "VCloud\\Helpers\\": "src/"
        }
    },
    "include-path": [
        "src/"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.2",
        "vmware/vcloud-sdk-patched": "550.3.*",
        "pear/http_request2": "2.2.*"
    },
    "require-dev": {
        "amercier/cli-helpers": "1.3.*",
        "pear/log": "*",
        "squizlabs/php_codesniffer": "1.5.*",
        "phpdocumentor/phpdocumentor": "2.3.0",
        "phpunit/phpunit": "4.0.*",
        "satooshi/php-coveralls": "0.6.*",
        "zendframework/zend-config": "2.3.*",
        "zendframework/zend-json": "2.3.*",
        "zendframework/zend-loader": "2.3.*"
    }
}
