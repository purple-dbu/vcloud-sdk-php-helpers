{
    "name": "purple-dbu/vcloud-sdk-helpers",
    "description": "Utility classes for vCloud Director PHP SDK",
    "type": "library",
    "keywords": [
        "vmware",
        "vcloud",
        "director",
        "api",
        "sdk",
        "helpers"
    ],
    "homepage": "https://github.com/purple-dbu/vcloud-sdk-php-helpers",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "VCloud\\Helpers\\": "src/"
        }
    },
    "include-path": [
        "src/"
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "vmware/vcloud-sdk-patched": "550.3.*",
        "pear-pear/Log": "1.12.*",
        "pear-pear/HTTP_Request2": "2.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "0.6.*",
        "pear-pear/PHP_CodeSniffer": "1.4.*",
        "zendframework/zend-config": "2.2.*",
        "zendframework/zend-json": "2.2.*",
        "zendframework/zend-loader": "2.2.*"
    }
}
